{
  "name": "@tsuredure-sns/core",
  "type": "module",
  "version": "0.0.1",
  "description": "Frontend for Tsuredure SNS - Core",
  "license": "MIT",
  "keywords": [
    "SNS",
    "WebRTC",
    "Sqlite"
  ],
  "homepage": "https://cafe-capy.net/tsuredure-sns/",
  "bugs": {
    "url": "https://github.com/tsuredure-sns/tsuredure-sns/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuredure-sns/tsuredure-sns"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "dist/index.iife.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "umd": "./dist/index.umd.js",
      "iife": "./dist/index.iife.js"
    },
    "./*": "./*",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "tsdown": "^0.15.1"
  },
  "scripts": {
    "build": "tsdown"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./*": "./*"
    }
  }
}
